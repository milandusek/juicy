{
  "scope": "juicyfx",
  "name": "avatarless",
  "regions": ["bru"],
  "rewrites": [
    { "source": "/favicon.ico", "destination": "https://juicy.vercel.app/favicon.ico" },
    { "source": "/g/(.+@.+\\..+)", "destination": "/api/gravatar.ts?_email=$1" },
    { "source": "/i/(.+)", "destination": "/api/initials.ts?_initials=$1" },
    { "source": "/gi/(.+@.+\\..+)", "destination": "/api/gravatar+initials.ts?_email=$1" },
    { "source": "/(.*)", "destination": "https://juicy.vercel.app/readme/avatarless" }
  ]
}
